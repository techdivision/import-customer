        </service>
        <service id="import_customer.action.processor.customer.update" class="TechDivision\Import\Dbal\Collection\Actions\Processors\GenericProcessor">
            <argument type="service" id="connection"/>
            <argument type="service" id="import_customer.repository.sql.statement"/>
